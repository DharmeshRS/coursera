<!DOCTYPE html>

<head>

    <title>sign In</title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta2/css/all.min.css">

    <link rel="stylesheet" href="/static/users/login.css">
</head>
<body>
    <div class="signin-form">
        <h1>Welcome Back</h1>
        <form action="/login" method="POST" id="myform">
            <p class="title">EMAIL</p>
            <input type = "email" id="email" name="email" placeholder = " Email" />
            <p class="title">PASSWORD</p>
            <input type = "text" id="password" name="password" placeholder = " Password" />
            <p class="title">Reset password or email me a login link</p>
            <input type="submit" id="signin-btn" value="Log In" >
            <div class="line">
                <div class="hr"><hr> </div>  <div><p class="or">or</p></div>  <div class="hr"><hr> </div>
            </div>

            <div class="icons">
                <div class="fab fa-google"></div>
                <div class="text">Continue with Google</div>
             </div>
            <div class="icons">
                <div class="fab fa-facebook-square"></div>
                <div class="text">Continue with Facebook</div>
             </div>
            <div class="icons">
                <div class="fab fa-apple"></div>
                <div class="text">Continue with Apple</div>
             </div>
         
        </form>
    </div>
</body>
</html>
<script src = "/static/users/login.js"></script>
